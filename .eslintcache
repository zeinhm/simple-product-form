{()=>handleCheckboxDiskon()}\n        />\n      </div>\n      {useDiskon &&\n      <>\n        <div className='diskon-container'>\n          <p>Diskon Produk</p>\n          {renderField('discount', ' ', onlyNums, 3)}\n          <p>%</p>\n        </div>\n        {formProduct['add-product'].values.discount && <div className='summary-diskon'>\n          <span>\n            <p>Harga diskon produk</p>\n            <p>-Rp{thousand((formProduct['add-product'].values.price*formProduct['add-product'].values.discount)/100)}</p>\n          </span>\n          <span>\n            <p>Harga produk setelah diskon</p>\n            <p>Rp{thousand((formProduct['add-product'].values.price*(100-formProduct['add-product'].values.discount))/100)}</p>\n          </span>\n        </div>}\n      </>}\n      {/* {(message !== 'SUKSES' && submitSucceeded) && <p className={styles.error}>{message}</p>} */}\n      <footer>\n        <ButtonLink to={linkBack}>Batal</ButtonLink>\n        <Button onSubmit={handleSubmit}>Simpan</Button>\n      </footer>\n    </form>\n  );\n}\n\nAddProductForm.defaultProps = {\n  change: () => { },\n  handleSubmit: () => { },\n  id: '',\n  invalid: true,\n  isLoadingUpdate: false,\n  nameCategory: '',\n  product: {},\n  submitSucceeded: false\n};\n\nAddProductForm.propTypes = {\n  change: PropTypes.func,\n  handleSubmit: PropTypes.func,\n  id: PropTypes.string,\n  invalid: PropTypes.bool,\n  isLoadingUpdate: PropTypes.bool,\n  nameCategory: PropTypes.string,\n  product: PropTypes.object,\n  submitSucceeded: PropTypes.bool\n};\n",{"ruleId":"124","replacedBy":"125"},{"ruleId":"126","replacedBy":"127"},{"ruleId":"128","severity":1,"message":"129","line":11,"column":1,"nodeType":"130","endLine":13,"endColumn":3},{"ruleId":"124","replacedBy":"131"},{"ruleId":"126","replacedBy":"132"},{"ruleId":"133","severity":1,"message":"134","line":1,"column":40,"nodeType":"135","messageId":"136","endLine":1,"endColumn":48},{"ruleId":"133","severity":1,"message":"137","line":2,"column":2